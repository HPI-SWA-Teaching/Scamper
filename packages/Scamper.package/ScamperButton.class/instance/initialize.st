initialize

	super initialize.
	IconicButton new
		target: self;
		color: (Color transparent);
		extent: 35@35;
		borderWidth: 0;
		borderColor: (Color transparent);
		actWhen: #buttonUp.		

		
		super color: (Color transparent).
		super borderWidth: 0.
		super borderColor: (Color transparent).
		super actWhen: #buttonUp.